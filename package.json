{
    "name": "gatsbystarter",
    "version": "1.0.0",
    "private": true,
    "description": "Ford Insurance",
    "author": "Baloise",
    "workspaces": [
        "wl"
    ],
    "keywords": [
        "gatsby"
    ],
    "scripts": {
        "cleandev": "gatsby clean && yarn develop",
        "cleanbuild": "yarn clean && yarn build",
        "develop": "yarn copy:fonts && gatsby develop -p 8005",
        "dev": "yarn cleandev",
        "start": "gatsby develop",
        "build": "gatsby clean && yarn copy:fonts && gatsby build --prefix-paths",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "copy:all": "yarn copy:fonts && yarn copy:broker-assets",
        "copy:fonts": "copyfiles --flat ../../node_modules/@baloise/design-system-fonts/lib/* public/assets/fonts",
        "copy:broker-assets": "copyfiles --flat ../../node_modules/@baloise-cfa/web-components/dist/web-components/assets/* public/assets",
        "modules": "typed-scss-modules src --watch",
        "debug": "source bin/init && /Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222 && gatsby develop"
    },
